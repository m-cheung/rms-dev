{
  "name": "rms",
  "version": "0.0.0",
  "description": "University of Waterloo's Campus Response Team Shift Scheduling System",
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "lint": "eslint -c .eslintrc public api",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "test": "karma start",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-cheung/rms-dev.git"
  },
  "author": "Michael Cheung <michael.cheung@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m-cheung/rms-dev/issues"
  },
  "homepage": "https://github.com/m-cheung/rms-dev#readme",
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./public",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./public",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./public",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "body-parser": "^1.14.1",
    "cas": "0.0.3",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "file-loader": "^0.10.1",
    "hoist-non-react-statics": "^1.0.3",
    "http-proxy": "^1.12.0",
    "invariant": "^2.2.0",
    "jsonwebtoken": "^7.1.9",
    "less": "^2.5.3",
    "less-loader": "^4.0.0",
    "lru-memoize": "^1.0.0",
    "map-props": "^1.0.0",
    "moment": "^2.16.0",
    "multireducer": "^3.1.0",
    "pg": "^6.1.0",
    "pg-pool": "^1.5.0",
    "piping": "^0.3.0",
    "pretty-error": "^2.0.3",
    "react": "15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-cookie": "^1.0.4",
    "react-dom": "15.4.2",
    "react-helmet": "^4.0.0",
    "react-inline-css": "^2.0.0",
    "react-redux": "^5.0.3",
    "react-router": "4.0.0",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.0.4",
